<% content_for(:navbar) do %>
<ul class="right">
  <li>Hi, <%= current_user.name %>!</li>
  <li><%= link_to "Log Out", log_out_path %></li>
</ul>
<% end %>


<h1>Questions</h1>

<ul>
  <% @questions.each do |question| %>
  <li><%= link_to question.name, user_question_path(current_user.id, question.id) %></li>
  <li><%= question.description %></li>

  <% end %>
</ul>

<%= link_to "new Question", new_user_question_path(current_user.id) %>
