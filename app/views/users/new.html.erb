<%=form_for User.new do |f| %>

<%=f.label :email %>

<%=f.text_field :email %>

<%=f.label :password %>

<%=f.password_field :password %>

<%=f.label :password_confirmation %>

<%=f.password_field :password_confirmation %>

<%=f.label :name %>

<%=f.text_field :name %>


<%=f.submit "Sign Up" %>

<%end%>
